{
  "ignoreFiles": [
    "**/obj/**/*.fs",
    "**/bin/**/*.fs",
    "**/.ionide/**/*.fs"
  ],
  "global": {
    "numIndentationSpaces": 4
  },
  "hints": {
    "enabled": true
  },
  "cyclomaticComplexity": {
    "enabled": true,
    "config": {
      "maxComplexity": 15
    }
  },
  "typography": {
    "enabled": true,
    "indentation": {
      "enabled": true
    },
    "maxCharactersOnLine": {
      "enabled": false
    },
    "trailingWhitespaceOnLine": {
      "enabled": true,
      "numberOfSpacesAllowed": 0
    },
    "maxLinesInFunction": {
      "enabled": false
    },
    "trailingNewLineInFile": {
      "enabled": false
    }
  },
  "sourceLength": {
    "enabled": true,
    "maxLinesInFunction": {
      "enabled": false
    },
    "maxLinesInLambdaFunction": {
      "enabled": false
    },
    "maxLinesInMatchLambdaFunction": {
      "enabled": false
    },
    "maxLinesInValue": {
      "enabled": false
    },
    "maxLinesInConstructor": {
      "enabled": false
    },
    "maxLinesInMember": {
      "enabled": false
    },
    "maxLinesInProperty": {
      "enabled": false
    },
    "maxLinesInModule": {
      "enabled": false
    },
    "maxLinesInRecord": {
      "enabled": false
    },
    "maxLinesInEnum": {
      "enabled": false
    },
    "maxLinesInUnion": {
      "enabled": false
    },
    "maxLinesInClass": {
      "enabled": false
    },
    "maxCyclomaticComplexityForFunction": {
      "enabled": true,
      "config": {
        "maxComplexity": 15
      }
    }
  },
  "formatting": {
    "enabled": true,
    "typedItemSpacing": {
      "enabled": false,
      "config": {
        "typeDefinitionSpacing": "SpaceAfter"
      }
    },
    "unionDefinitionIndentation": {
      "enabled": true
    },
    "moduleDeclSpacing": {
      "enabled": false
    },
    "classMemberSpacing": {
      "enabled": true
    }
  },
  "numberOfItems": {
    "enabled": true,
    "maxNumberOfFunctionParameters": {
      "enabled": true,
      "config": {
        "maxItems": 5
      }
    },
    "maxNumberOfMembers": {
      "enabled": false
    },
    "maxNumberOfItemsInTuple": {
      "enabled": true,
      "config": {
        "maxItems": 4
      }
    },
    "maxNumberOfBooleanOperatorsInCondition": {
      "enabled": true,
      "config": {
        "maxItems": 4
      }
    },
    "maxCyclomaticComplexity": {
      "enabled": true,
      "config": {
        "maxComplexity": 15
      }
    }
  },
  "conventions": {
    "enabled": true,
    "naming": {
      "enabled": true,
      "interfaceNames": {
        "enabled": true,
        "config": {
          "naming": "PascalCase",
          "prefix": "I"
        }
      },
      "typeNames": {
        "enabled": true,
        "config": {
          "naming": "PascalCase"
        }
      },
      "recordFieldNames": {
        "enabled": true,
        "config": {
          "naming": "PascalCase"
        }
      },
      "enumCasesNames": {
        "enabled": true,
        "config": {
          "naming": "PascalCase"
        }
      }
    },
    "recursiveAsyncFunction": {
      "enabled": true
    },
    "redundantNewKeyword": {
      "enabled": true
    },
    "nestedStatements": {
      "enabled": false
    },
    "reimplementsFunction": {
      "enabled": true
    },
    "canBeReplacedWithComposition": {
      "enabled": true
    }
  }
}
